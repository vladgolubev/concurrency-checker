{
  "name": "concurrency",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib/ && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts'",
    "lint": "eslint . --ext .js,.ts,.json --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml,graphql}": [
      "prettier --write"
    ],
    "*.{ts,js,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "resolutions": {
    "aws-sdk": "2.1055.0"
  },
  "dependencies": {
    "p-map": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/node": "7.20.7",
    "@shelf/babel-config": "1.2.0",
    "@shelf/eslint-config": "2.27.1",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.0.9",
    "@types/aws-lambda": "8.10.109",
    "@types/node": "18",
    "aws-sdk": "2.1055.0",
    "eslint": "8.32.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.5",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  }
}
