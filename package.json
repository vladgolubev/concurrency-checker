{
  "name": "@shelf/shelf-api-xxx",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/shelfio/shelf-api-xxx#readme",
  "repository": "shelfio/shelf-api-xxx",
  "license": "UNLICENSED",
  "author": "Vitalii Sikora",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib/ && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts'",
    "coverage": "jest --coverage",
    "deploy": "serverless deploy --stage $ENVIRONMENT --package .serverless",
    "lint": "eslint . --fix --ext .js,.json,.ts --quiet",
    "lint:ci": "eslint . --ext .js,.json,.ts --quiet",
    "package": "serverless package --stage $ENVIRONMENT",
    "test": "TZ=UTC jest src",
    "test:integration": "TZ=UTC jest api-tests",
    "test:integration:staging": "ENVIRONMENT=staging API_HOST=api.gsstaging.net yarn test:integration",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,json,md,yml,graphql}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/common-mocks.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@shelf/aws-sdk-helpers": "3.9.12",
    "@shelf/eslint-config": "0.14.2",
    "@types/jest": "25.1.3",
    "@types/node": "12",
    "aws-sdk": "2.536.0",
    "babel-jest": "25.1.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "serverless-associate-waf": "1.2.0",
    "serverless-iam-roles-per-function": "2.0.2",
    "typescript": "3.7.5"
  },
  "resolutions": {
    "aws-sdk": "2.536.0",
    "lodash": "4.17.15"
  }
}
