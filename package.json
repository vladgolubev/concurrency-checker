{
  "name": "@shelf/shelf-api-xxx",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/shelfio/shelf-api-xxx#readme",
  "repository": "shelfio/shelf-api-xxx",
  "license": "UNLICENSED",
  "author": "Vitalii Sikora",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib/ && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts'",
    "coverage": "yarn test --coverage",
    "deploy": "serverless deploy --package .serverless",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint . --ext .js,.json,.ts --quiet",
    "package": "serverless package",
    "test": "TZ=UTC jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml,graphql}": [
      "prettier --write"
    ],
    "*.{ts,js,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/schema.ts",
      "!src/**/types.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/common-mocks.js"
    ]
  },
  "resolutions": {
    "aws-sdk": "2.1001.0",
    "lodash": "4.17.21"
  },
  "dependencies": {
    "@middy/core": "2.5.7",
    "@shelf/logger": "5.0.0",
    "@shelf/middy-base": "3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.0",
    "@babel/node": "7.16.8",
    "@shelf/aws-sdk-helpers": "5.4.185",
    "@shelf/babel-config": "0.1.8",
    "@shelf/eslint-config": "2.12.1",
    "@shelf/lambda-test-utils": "4.0.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/serverless-simplify-default-exec-role-plugin": "0.0.1",
    "@shelf/tsconfig": "0.0.6",
    "@types/aws-lambda": "8.10.92",
    "@types/jest": "27.4.0",
    "@types/node": "14",
    "aws-sdk": "2.1001.0",
    "babel-jest": "27.4.6",
    "eslint": "8.8.0",
    "http-errors": "2.0.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "serverless-associate-waf": "1.2.1",
    "serverless-iam-roles-per-function": "3.2.0",
    "serverless-tag-cloud-watch-log-groups": "1.2.5",
    "typescript": "4.5.5"
  }
}
